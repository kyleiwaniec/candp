<script type="text/javascript">
function getproperties(price){
	params="destination=Dublin%2C+Ireland"

	$(function() {
	url="/getbyfilter.php?"+ params+"&minRate=&maxRate=&minStarRating=&maxStarRating=&minprice=&maxprice=&minrating=&maxrating="
		$.ajax({
		type: "GET",
		url: url,
		success: function(data)
		{
			$(".accomm_details").append(data)
		},
		error: function() {
                alert("Oops - something went wrong!")
            }
	});
	
	});

}
getproperties(1);
scrolling=false
$(window).scroll(function() {
getMoreResults()

});
function getMoreResults(){
if(scrolling==false && $("#nextpage").attr("data-href")!=""){
   if($(window).scrollTop() + $(window).height() > $(document).height() - 400) {
	scrolling=true
	
	$.ajax({
		url: "/getbyfilter.php?destination=Dublin%2C+Ireland&"+$("#nextpage").attr("data-href")+"&minRate=&maxRate=&minStarRating=&maxStarRating=&minprice=&maxprice=&minrating=&maxrating=",
		type: "GET",
		cache: false,
		success: function(data)
		{
			$("#nextpage").remove()
			$(".oops").remove()
			$('.accomm_details').append(data) 
			scrolling=false;
		},
		error: function(){
		}
	}); 
   }

   }
}
$(".sort li a").click(function(e){

	if($("#nextpage").attr("data-href")!=""){

		dosort($(this))
	}
	else
	{

$(".accomm_details").fadeTo('fast', 0.2)
	//$(".modal").show();
	//priceRange=getPriceRange()
	//starRange=getStarRange()
	//alert("/getbyfilter.php?destination=Dublin%2C+Ireland&sortby="+$(this).attr("data-sort")+"&minRate=&maxRate=&minStarRating=&maxStarRating=&minprice=&maxprice=&minrating=&maxrating=")
	$.ajax({
		url: "/getbyfilter.php?destination=Dublin%2C+Ireland&sortby="+$(this).attr("data-sort")+"&minRate=&maxRate=&minStarRating=&maxStarRating=&minprice=&maxprice=&minrating=&maxrating=",
		type: "GET",
		cache: false,
		success: function(data)
		{
			$('.accomm_details').html(data) 
			$(".accomm_details").fadeTo('slow', 1)
			//$(".modal").hide();
		},
		error: function(){
			$(".accomm_details").fadeTo('slow', 1)
			//$(".modal").hide();
		}
	}); 
}
})
 //$(".one-fourth:nth-child(3n)").css("margin-right", "0");
function dosort(e){
$("#nextpage").hide()
if($(e).attr("data-sort")=="PRICE"){
	sort="asc";sortby="data-price";
}
else if($(e).attr("data-sort")=="PRICE_REVERSE"){
	sort="desc";sortby="data-price";
}
else if($(e).attr("data-sort")=="QUALITY_REVERSE"){
	sort="asc";sortby="data-star";
}
else if($(e).attr("data-sort")=="QUALITY"){
	sort="desc";sortby="data-star";
}
else if($(e).attr("data-sort")=="PROMO"){
	sort="desc";sortby="data-promo";
}
else if($(e).attr("data-sort")=="OVERALL_VALUE"){
	sort="asc";sortby="data-recommended";
}
$.when(
    $(".accomm_details").jSort({
        sort_by: 'article',
        item: 'article',
        order: sort,
		is_num:true,
        sort_by_attr: true,
        attr_name: sortby
    })).then(function(){
	//alert("K")
	//$(".one-fourth").css('margin-right', '');
    //    $(".one-fourth:nth-child(3n)").css("margin-right", "0");
	}
	);



}
</script>
